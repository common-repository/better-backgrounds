#@ bbg
#: bbg_admin.php:449
msgid "Better Background"
msgstr "Backgrounds Melhores"

#@ bbg
#: bbg_admin.php:449
msgid "Recommended"
msgstr "Recomendado"

#@ bbg
#: bbg_admin.php:451
msgid "Easiest to implement and offers the most options and customizations. Scales it to fit browser window exactly."
msgstr "O jeito mais fácil de implementar e oferece vastas opções e personalizações. Adapta-se à escala exata do navegador."

#@ bbg
#: bbg_admin.php:454
msgid "Classic Background"
msgstr "Fundo clássico"

#@ bbg
#: bbg_admin.php:456
msgid "Uses the standard body background css attribute. Does not scale the background to fit the page's width or support fade in. Works like the regular Wordpress Custom Background functionality."
msgstr "Usa o atributo CSS padrão para fundo de corpo. Não escala o fundo para adaptar-se à largura da página ou suporta desvanecimento. Trabalha como um fundo personalizado normal do WordPress."

#@ bbg
#: bbg_admin.php:459
msgid "Developer"
msgstr "Desenvolvedor"

#@ bbg
